{
	"name": "nextjs-starter-kit",
	"version": "1.0.0",
	"license": "ISC",
	"private": true,
	"browserslist": [
		"defaults",
		"last 50 versions",
		"not IE 11",
		"> 0.5%"
	],
	"repository": {
		"type": "git",
		"url": "https://gitlab.aeroidea.ru/aero/nextjs-starter-kit.git"
	},
	"scripts": {
		"dev": "yarn variables && next dev",
		"build": "next build",
		"lint": "tsc && next lint --fix",
		"prettier": "prettier -c \"src/**/*.{ts,tsx,graphql}\"",
		"start": "NODE_ENV=production next start",
		"analyze": "ANALYZE=true next build",
		"favicons": "real-favicon generate src/assets/meta/meta-description.json src/assets/meta/meta-data.json ./public/meta && cpy public/meta/favicon.ico public",
		"variables": "node ./src/shared/tools/create-variables.js",
		"icons": "npx ts-node ./src/shared/tools/create-icons.ts",
		"new:entity": "hygen new entity",
		"new:feature": "hygen new feature",
		"new:widget": "hygen new widget"
	},
	"dependencies": {
		"@next/bundle-analyzer": "14.0.4",
		"@reduxjs/toolkit": "2.0.1",
		"@tanstack/react-query": "5.15.0",
		"@tanstack/react-query-devtools": "5.15.0",
		"axios": "1.6.3",
		"classnames": "2.4.0",
		"cookies-next": "4.1.0",
		"dotenv": "16.3.1",
		"html-react-parser": "5.0.11",
		"next": "14.0.4",
		"next-pwa": "5.6.0",
		"next-redux-wrapper": "8.1.0",
		"normalize.css": "8.0.1",
		"qs": "6.11.2",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-redux": "9.0.4",
		"react-toastify": "9.1.3",
		"redux": "5.0.1",
		"redux-thunk": "3.1.0",
		"sass": "1.69.5",
		"swiper": "11.0.5"
	},
	"devDependencies": {
		"@next/eslint-plugin-next": "14.0.4",
		"@svgr/webpack": "8.1.0",
		"@tanstack/eslint-plugin-query": "5.14.6",
		"@types/node": "20.10.5",
		"@types/qs": "6.9.11",
		"@types/react": "18.2.45",
		"@types/react-redux": "7.1.33",
		"@typescript-eslint/eslint-plugin": "6.16.0",
		"@typescript-eslint/parser": "6.16.0",
		"cli-real-favicon": "0.0.8",
		"cpy-cli": "5.0.0",
		"eslint": "8.56.0",
		"eslint-config-airbnb-typescript": "17.1.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-import": "2.29.1",
		"eslint-plugin-react": "7.33.2",
		"eslint-plugin-react-hooks": "4.6.0",
		"eslint-plugin-simple-import-sort": "10.0.0",
		"hygen": "6.2.11",
		"prettier": "^3.2.2",
		"typescript": "5.3.3"
	}
}
